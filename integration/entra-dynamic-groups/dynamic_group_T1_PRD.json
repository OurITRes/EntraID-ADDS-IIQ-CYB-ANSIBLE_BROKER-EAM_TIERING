{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "type": "EntraID-DynamicGroup-Template",
  "name": "GRP-DYN-T1-PRD-Users",
  "properties": {
    "displayName": "GRP-DYN-T1-PRD-Users",
    "description": "Dynamic group: users with TIER=T1 and ENV=PRD",
    "membershipRule": "(user.extensionAttribute10 -eq \"T1\") and (user.extensionAttribute11 -eq \"PRD\")",
    "membershipRuleProcessingState": "On",
    "securityEnabled": true,
    "mailEnabled": false,
    "groupTypes": [
      "DynamicMembership"
    ]
  }
}